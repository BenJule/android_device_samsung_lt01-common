# at_distributor
type at_distributor, domain;
type at_distributor_exec, exec_type, file_type;

init_daemon_domain(at_distributor)

allow at_distributor serial_device:chr_file rw_file_perms;
allow at_distributor secril-daemon:unix_stream_socket connectto;
allow at_distributor efs_file:file { read open getattr setattr };
allow at_distributor self:process execmem;
allow at_distributor self:capability { chown fowner setuid };
